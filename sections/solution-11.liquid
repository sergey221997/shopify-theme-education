{{ 'exercise-11.css' | asset_url | stylesheet_tag }}

{% render 'exercise-11' %} 

<table class="table">
    {% for multiplier in (1..10) %}
        <tr class="table-row {% if forloop.first %}mod{% endif %}">
        {% for factor in (1..10) %} 
            <td class="table-data {% if forloop.first %}mod{% endif %}">
                {{ multiplier | times: factor }}
            </td>
        {% endfor %} 
      </tr>
    {% endfor %}
</table>

{% render 'exercise-wrapper', Nav: Nav, Content: Content %}

{% schema %}
    {
        "name": "Exercise 11",
        "limit": 1,
        
        "presets": [
            {
                "name": "Exercise 11"
            }
        ],
        "enabled_on": {  
            "templates": ["index"]
       }
    }
{% endschema %}