{% render 'exercise-9' %}

{% assign num_list1 = '1,3,4,7,2' %}
{% assign num_list2 = '76,2,99,6,8' %}

{% assign num_list1_array = num_list1 | split: ',' %}
{% assign num_list2_array = num_list2 | split: ',' %}

{% assign result_string = '' %}

{% for item1 in num_list1_array %}
    {% assign one = item1 %}
    {% assign two = num_list2_array[forloop.index0] %}
    {% assign sum = one | plus: two %}
    {% assign result_string = result_string | append: sum %}
    {% if forloop.last == false %}
        {% assign result_string = result_string | append: ',' %}
    {% endif %}
{% endfor %}

{{ result_string }}

{% render 'exercise-wrapper', Nav: Nav, Content: Content %}

{% schema %}
    {
        "name": "Exercise 9",
        "limit": 1,
        
        "presets": [
            {
                "name": "Exercise 9"
            }
        ],
        "enabled_on": {  
            "templates": ["index"]
       }
    }
{% endschema %}